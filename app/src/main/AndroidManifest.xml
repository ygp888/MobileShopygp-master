<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ygp">
<uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:name="com.example.ygp.common.MobileShopApp"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/shop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shop"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ygp.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ygp.activity.AdActivity"/>
        <activity android:name="com.example.ygp.activity.MainActivity"/>
        <activity android:name="com.example.ygp.activity.LoginActivity"/>
        <activity android:name="com.example.ygp.activity.MyOrderActivity"/>
        <activity android:name="com.example.ygp.activity.MyCollectActivity"/>
        <activity android:name="com.example.ygp.activity.MyAddressActivity"/>
        <activity android:name="com.example.ygp.activity.ChangePwdActivity"/>
        <activity android:name="com.example.ygp.activity.GoodsListActivity"/>
        <activity android:name="com.example.ygp.activity.GoodsDetailActivity"/>
    </application>

</manifest>